RewriteEngine on
# RewriteRule .* public/$1 [L]
# RewriteRule    ^$    webroot/    [L]
# RewriteRule    (.*) $1    [L]
# RewriteRule ^mail/test-send(\.html)?$ index.php?controller=mail&action=test_send [NC,L]
RewriteRule ^(error)/(page-introuvable).html$ index.php?controller=$1&action=$2 [NC,L]
RewriteRule ^([a-zA-Z]*)/?([a-zA-Z]*)?-?([a-zA-Z0-9]*)?-?(\.html)?$ index.php?controller=$1&action=$2&apikey=$3 [NC,L]
RewriteRule ^utilitaire/(publipostage)/([a-zA-Z0-9]*)-(voir).html$ index.php?controller=$1&action=$3&uuid=$2 [NC,L]
ErrorDocument 404 http://localhost/portfolio/frontoffice/services/error/page-introuvable.html
